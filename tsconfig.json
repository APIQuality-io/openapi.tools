{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "paths": {
      "@/components/*": ["./src/components/*"],
      "@/layouts/*": ["./src/layouts/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "jsx.d.ts",
    "env.d.ts",
    "types.d.ts",
    "posthog.d.ts"
  ]
}
